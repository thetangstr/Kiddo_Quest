{
  "expo": {
    "name": "KiddoQuest",
    "slug": "kiddoquest",
    "version": "1.0.0",
    "privacy": "public",
    "description": "KiddoQuest - A fun and engaging family chore management app that turns everyday tasks into exciting quests for kids.",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kiddoquest.app",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "1",
      "infoPlist": {
        "UIBackgroundModes": ["background-fetch"],
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app uses the camera to let kids upload photos of completed quests.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photo library to let kids choose photos for their profiles and quest completions."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kiddoquest.app",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "CAMERA",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [],
    "extra": {
      "eas": {
        "projectId": "b1de6edc-e799-44d9-bce3-c9eedf49b27d"
      }
    }
  }
}
